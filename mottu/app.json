{
  "expo": {
    "name": "mottu",
    "slug": "mottu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mottu-logo.png",
    "scheme": "mottu",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIStatusBarHidden": true,
        "UIViewControllerBasedStatusBarAppearance": false
      },
      "bundleIdentifier": "com.tcguus.mottu"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mottu-logo.png",
        "backgroundColor": "#00B131"
      },
      "package": "com.tcguus.mottu",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "useNextNotificationsApi": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDZ5vx6MefKhbGE3C8-vZMwtAGZUVSSozY"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mottu-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00B131"
        }
      ],
      "expo-notifications",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bf144db0-92a7-4b3a-a4a2-f1abeb4e5aea"
      }
    }
  }
}